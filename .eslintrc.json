{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "extends": [
        "eslint:recommended"
    ],
    "rules": {
        "semi": 2,
        "no-const-assign": 2,
        "no-console": 0
    },
    "env": {
        "browser": true,
        "node": true
    },
    "globals": {
        "Uint8Array": true,
        "Float32Array": true,
        "Promise": true
    }
}